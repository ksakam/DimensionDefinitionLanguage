diff tmp tmp2
bash cat-example.script | ./tq -FT in=/dev/stdin -C > tmp.o
bash cat-example.script | ./tq -FT in=/dev/stdin -C | ./tq -FT in=/dev/stdin -C > tmp2.o
diff tmp.o tmp2.o
